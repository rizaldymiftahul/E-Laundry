<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.e_laundry.user.e_laundry.kontak">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"
        android:id="@+id/relativeLayout">

        <TextView
            android:id="@+id/textKontak"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="KONTAK KAMI"
            android:textAlignment="center"
            android:textSize="30dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="93dp" />

    </RelativeLayout>



    <TextView
        android:id="@+id/textNama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="E LAUNDRY"
        android:background="@color/colorPrimary"
        android:textAlignment="center"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="88dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textTelp1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No.Telp 082245230049"
        android:textAlignment="center"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="138dp"
        android:layout_below="@+id/relativeLayout"
        android:layout_alignRight="@+id/textAlamat"
        android:layout_alignEnd="@+id/textAlamat"
        android:layout_marginTop="20dp" />

    <TextView
        android:id="@+id/textTelp2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(321)234 553"
        android:textAlignment="center"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="171dp"
        android:layout_below="@+id/textTelp1"
        android:layout_alignRight="@+id/textTelp1"
        android:layout_alignEnd="@+id/textTelp1"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="10dp" />

    <TextView
        android:id="@+id/textAlamat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ALAMAT KAMI"
        android:textAlignment="center"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="227dp"
        android:layout_below="@+id/textTelp2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="56dp" />

    <TextView
        android:id="@+id/textAlamat1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jl.Sumatra gang 11, Sumbersari, Jember"
        android:textAlignment="center"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="265dp"
        android:layout_below="@+id/textAlamat"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="24dp" />



    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/logomat"
        android:layout_marginTop="62dp"
        android:layout_alignTop="@+id/textAlamat1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>

