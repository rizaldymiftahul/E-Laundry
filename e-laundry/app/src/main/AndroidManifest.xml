<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.e_laundry.user.e_laundry">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".app.Controller"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login" />
        <activity android:name=".Forgot" />
        <activity android:name=".Register" />
        <activity
            android:name=".navigation"
            android:label="E-laundry"
            android:theme="@style/AppTheme" />
        <activity android:name=".alamat" />
        <activity android:name=".kontak" />
        <activity android:name=".Profil" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".proses" />
        <activity android:name=".detail_pesanan"></activity>
    </application>

</manifest>